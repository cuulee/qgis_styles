              <layer pass="0" class="GeometryGenerator" locked="0">
                <prop k="SymbolType" v="Line"/>
                <prop k="geometryModifier" v="$geometry"/>
                <symbol alpha="1" clip_to_extent="1" type="line" name="@@@Distance Measurement@0@0@1">
